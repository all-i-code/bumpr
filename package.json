{
  "name": "bumpr",
  "version": "1.4.0",
  "description": "Bump the version of an package based on a Pull Request",
  "bin": {
    "bumpr": "./bin/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:jobsquad/bumpr.git"
  },
  "engines": {
    "node": ">= 9.0"
  },
  "keywords": [
    "github",
    "pull-request",
    "version",
    "semver",
    "bump"
  ],
  "author": "Adam Meadows [https://github.com/job13er]",
  "contributors": [
    "Matthew Dahl [https://github.com/sandersky]",
    "Steven Glanzer [https://github.com/sglanzer]",
    "Zak Henry [https://github.com/zakhenry]"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jobsquad/bumpr/issues"
  },
  "homepage": "https://github.com/jobsquad/bumpr#readme",
  "scripts": {
    "precommit": "lint-staged",
    "lint": "eslint *.js bin src",
    "test": "yarn run lint && jest --coverage"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --config .prettierrc.js --write",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/typedefs.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "statements": 100
      }
    },
    "testEnvironment": "node"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "commander": "^2.9.0",
    "lodash": "^4.0.1",
    "nlf": "^1.4.1",
    "node-fetch": "^1.3.3",
    "promise": "^7.1.1",
    "replace-in-file": "^3.4.0",
    "versiony": "^2.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier-airbnb": "^0.3.0",
    "eslint-plugin-import": "^2.11.0",
    "freezly": "^1.0.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "lint-staged": "^7.1.0",
    "prettier": "^1.12.1",
    "webpack": "^4.8.0",
    "webpack-cli": "^2.1.3",
    "webpack-node-externals": "^1.7.2"
  }
}